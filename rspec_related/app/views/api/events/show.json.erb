{
  "id": "<%= current_object.id %>",
  "name": "<%= current_object.name %>",
  "start_date": "<%= current_object.start_date.to_s %>",
  "start_time": "<%= current_object.start_time.strftime('%H:%M') %>",
  "end_date": "<%= current_object.end_date.to_s %>",
  "end_time": "<%= current_object.end_time.strftime('%H:%M') %>",
  "location": "<%= current_object.location %>",
  "private": "<%= current_object.private %>",
  "description": "<%= current_object.description %>",
  "instructions": "<%= current_object.instructions %>",
  "street": "<%= current_object.addresses.first.street rescue "" %>",
  "unit": "<%= current_object.addresses.first.unit rescue "" %>",
  "city": "<%= current_object.addresses.first.city rescue "" %>",
  "state": "<%= current_object.addresses.first.state rescue "" %>",
  "zip": "<%= current_object.addresses.first.zip rescue "" %>",
  "website": "<%= current_object.websites.first rescue "" %>",
  "phone": "<%= current_object.phone_numbers.first rescue "" %>"
}
